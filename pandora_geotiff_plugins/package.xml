<?xml version="1.0"?>
<package format="2">
  <name>pandora_geotiff_plugins</name>
  <version>0.0.1</version>
  <description>
    pandora_geotiff_plugins contains plugins that extend geotiff maps generated by pandora_geotiff.
  </description>

  <maintainer email="chamzask@gmail.com">Chamzas Konstantinos</maintainer>

  <license>BSD</license>

  <author>Chamzas Konstantinos</author>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>pluginlib</depend>
  <depend>nav_msgs</depend>
  <depend>tf</depend>
  <depend>pandora_geotiff</depend>
  <depend version_eq="geotiff_interfaces">pandora_data_fusion_msgs</depend>

  <export>
    <pandora_geotiff plugin="${prefix}/pandora_geotiff_plugins.xml" />
  </export>
</package>
